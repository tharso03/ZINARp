Package: ZINARp
Type: Package
Title: Simulate INAR/ZINAR(p) Models and Estimate Its Parameters
Version: 0.1.0
Authors@R: c(person(given = 'Aldo William', family = 'Medina Garay', email = 'aldo.garay@ufpe.br', role = 'aut'),
             person(given = 'Francyelle', family = 'de Lima Medina', email = 'francyelle.medina@ufpe.br', role = 'aut'),
